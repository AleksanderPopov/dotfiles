{
  "eslint.autoFixOnSave": true,
  "languageserver": {
    "phplang": {
      "command": "php",
      "args": ["vendor/bin/php-language-server.php"],
      "filetypes": ["php"]
    },
    "sql": {
      "command": "sql-language-server",
      "args": ["up", "--method", "stdio"],
      "filetypes": ["sql"]
    }
  }
}
